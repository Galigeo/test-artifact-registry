{
  "name": "test-artifactregistry",
  "version": "0.0.1",
  "description": "Quick test import dependencies from Google Artifact Registry",
  "main": "./dist/index.js",
  "scripts": {
    "artifactregistry-login": "npx google-artifactregistry-auth --repo-config=.npmrc --credential-config=.npmrc",
    "start": "node ./dist/index.js",
    "dev": "npx ts-node src/index.ts",
    "prebuild": "npm run login",
    "build": "echo running builld && tsc -p .",
    "gcp-build": "echo running gcp-build && tsc -p .",
    "deploy": "gcloud config set project retail-poi-service && gcloud app deploy",
    "login": "npx artifactregistry-auth",
    "preinstall": "npm run login && npm run artifactregistry-login",
    "prepare": "npm run login"
  },
  "author": "Galigeo",
  "license": "UNLICENSE",
  "dependencies": {
    "@galigeo-store/shared-models": "^0.0.4-beta",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "google-artifactregistry-auth": "^3.1.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.3"
  }
}